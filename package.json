{
  "name": "shapez",
  "version": "1.0.0",
  "description": "Shape drawing game for kids",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html -d dist",
    "deploy":
      "rm -rf dist && npm run build && workbox injectManifest workbox-config.js && echo shapez.euans.space > dist/CNAME && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/euangoddard/shapez.git"
  },
  "author": "Euan Goddard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/euangoddard/shapez/issues"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.7.0",
    "typescript": "^2.8.1",
    "workbox-cli": "^3.1.0"
  },
  "dependencies": {}
}
