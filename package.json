{
  "name": "shapez",
  "version": "1.0.0",
  "description": "Shape drawing game for kids",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html -d dist",
    "deploy": "rm -rf dist && npm run build && workbox injectManifest workbox-config.js && echo shapez.euans.space > dist/CNAME && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/euangoddard/shapez.git"
  },
  "author": "Euan Goddard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/euangoddard/shapez/issues"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.7.5",
    "workbox-cli": "^3.1.0"
  },
  "dependencies": {
    "src": "^1.1.2"
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 safari versions",
    "last 2 ios_saf versions"
  ]
}
